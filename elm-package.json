{
    "version": "2.7.1",
    "summary": "Elm Clarity UI is a wrapper library for the Clarity UI Framework",
    "repository": "https://github.com/mc706/elm-clarity-ui.git",
    "license": "MIT",
    "source-directories": [
        "./src/"
    ],
    "exposed-modules": [
        "ClarityUI.Alert",
        "ClarityUI.CDN",
        "ClarityUI.Grid",
        "ClarityUI.Icon",
        "ClarityUI.Card",
        "ClarityUI.Layout",
        "ClarityUI.Header",
        "ClarityUI.Subnav",
        "ClarityUI.Sidenav",
        "ClarityUI.Spinner",
        "ClarityUI.Button",
        "ClarityUI.ProgressBar",
        "ClarityUI.Badge",
        "ClarityUI.Label"
    ],
    "dependencies": {
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "elm-lang/html": "2.0.0 <= v < 3.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
